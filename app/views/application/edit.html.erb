<h2>edit comic</h2>
<form action="/update/<%= @comic.id %>">
  <input type="text" name="title" placeholder="title" value="<%= @comic.title %>">
  <input type="text" name="publisher" placeholder="publisher" value="<%= @comic.publisher %>">
  <input type="text" name="writer" placeholder="writer" value="<%= @comic.writer %>">
  <input type="text" name="cover" placeholder="cover" value="<%= @comic.cover %>">
  <input type="submit">
</form>